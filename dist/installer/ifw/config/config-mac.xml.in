    <Publisher>Canaan Inc.</Publisher>
    <ProductUrl>https://www.canaan.io</ProductUrl>

    <MaintenanceToolName>Uninstall CanMV IDE</MaintenanceToolName>
    <TargetDir>@homeDir@/Applications/CanMV IDE</TargetDir>
    <AllowSpaceInPath>true</AllowSpaceInPath>
    <ControlScript>controlscript.qs</ControlScript>
</Installer>
